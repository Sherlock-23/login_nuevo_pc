        <activity
            android:name=".login"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ImplicitActivity"
            android:screenOrientation="portrait"

            ></activity>

        <activity
            android:name=".Inicio"
            android:screenOrientation="portrait"

            ></activity>

        <activity
            android:name=".Principal"
            android:screenOrientation="portrait"

            ></activity>

        <activity
            android:name=".probando"
            android:screenOrientation="portrait" />

        <activity android:name=".menu_principal"
            android:screenOrientation="portrait" />

        <activity android:name=".admusuario"
            android:screenOrientation="portrait" />

        <activity android:name=".admmascota"
            android:screenOrientation="portrait" />

        <activity android:name=".integracion"
            android:screenOrientation="portrait" />

    </application>

</manifest>
